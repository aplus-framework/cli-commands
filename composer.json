{
    "name": "aplus/cli-commands",
    "type": "library",
    "description": "Aplus Framework CLI Commands Library",
    "keywords": [
        "cli",
        "commands"
    ],
    "homepage": "https://docs.aplus-framework.com/guides/libraries/cli-commands/",
    "license": "MIT",
    "authors": [
        {
            "name": "Natan Felles",
            "email": "natanfelles@gmail.com",
            "homepage": "https://natanfelles.github.io"
        }
    ],
    "require": {
        "php": ">=8.0",
        "aplus/mvc": "dev-master"
    },
    "require-dev": {
        "ext-xdebug": "*",
        "aplus/coding-standard": "^1.11",
        "ergebnis/composer-normalize": "^2.15",
        "phpmd/phpmd": "^2.10",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Framework\\CLI\\Commands\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\CLI\\Commands\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "email": "support@aplus-framework.com",
        "issues": "https://gitlab.com/aplus-framework/libraries/cli-commands/issues",
        "wiki": "https://gitlab.com/aplus-framework/libraries/cli-commands/wikis/home",
        "source": "https://gitlab.com/aplus-framework/libraries/cli-commands",
        "docs": "https://docs.aplus-framework.com/guides/libraries/cli-commands/"
    }
}
