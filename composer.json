{
	"name": "natanfelles/cli-commands",
	"type": "library",
	"description": "The Framework CLI Commands package",
	"keywords": [
		"cli",
		"commands"
	],
	"homepage": "https://the-framework.gitlab.io/libraries/cli-commands.html",
	"license": "proprietary",
	"authors": [
		{
			"name": "Natan Felles",
			"email": "natanfelles@gmail.com"
		}
	],
	"require": {
		"php": ">=8.0"
	},
	"require-dev": {
		"ext-xdebug": "*",
		"ergebnis/composer-normalize": "^2.0",
		"natanfelles/coding-standard": "^1.0",
		"phpmd/phpmd": "^2.9",
		"phpstan/phpstan": "^0.12",
		"phpunit/phpunit": "^9.3"
	},
	"autoload": {
		"psr-4": {
			"Framework\\CLI\\Commands\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\CLI\\Commands\\": "tests/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"scripts": {
		"post-update-cmd": [
			"composer dump-autoload"
		]
	},
	"support": {
		"email": "natanfelles@gmail.com",
		"issues": "https://gitlab.com/the-framework/libraries/cli-commands/issues",
		"wiki": "https://gitlab.com/the-framework/libraries/cli-commands/wikis/home",
		"source": "https://gitlab.com/the-framework/libraries/cli-commands",
		"docs": "https://the-framework.gitlab.io/libraries/cli-commands/docs/"
	}
}
